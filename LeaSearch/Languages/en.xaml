<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!--  MainWindow  -->
    <system:String x:Key="leaSearchWatermark">LeaSearch:Please input some word to start search</system:String>
    <system:String x:Key="registerHotkeyFailed">Failed to register hotkey: {0}</system:String>
    <system:String x:Key="couldnotStartCmd">Could not start {0}</system:String>

    <!--  notice  -->
    <system:String x:Key="notice_NoResult">Can not find any record</system:String>
    <system:String x:Key="notice_EnterChooseMode">Enter to select mode</system:String>
    <system:String x:Key="notice_SelectMode">Using↑/j/shift+tab move up， ↓/k /tab down，Enter doing default command，Space doing preview ，ESC back to input mode</system:String>

</ResourceDictionary>