<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!--  主窗体  -->
    <system:String x:Key="leaSearchWatermark">LeaSearch:请输入关键词开始查询</system:String>
    <system:String x:Key="registerHotkeyFailed">注册热键：{0} 失败</system:String>
    <system:String x:Key="couldnotStartCmd">启动命令 {0} 失败</system:String>

    <!--  提示  -->
    <system:String x:Key="notice_NoResult">找不到任何记录</system:String>
    <system:String x:Key="notice_EnterChooseMode">点击回车进入选择模式</system:String>
    <system:String x:Key="notice_SelectMode">使用↑/j/shift+tab 上移，使用↓/k /tab 下移，点击回车执行默认命令，ESC返回输入模式</system:String>

</ResourceDictionary>